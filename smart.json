{
  "global": {
    "color": {
      "white": "#ffffff",
      "black": "#000000",
      "pickled": "#364e66",
      "yellow_1": "#f39c1f",
      "yellow_2": "#f1c40f",
      "green_1": "#4bbba3",
      "green_2": "#3ca191",
      "blue_1": "#409ee0",
      "blue_2": "#2a82c1",
      "red_1": "#e67e2f",
      "red_2": "#d3540f"
    },
    "files": {
      "dist": "dist",
      "dest": "assets",
      "source": "src",
      "concatenator": "-"
    },
    "default": {
      "none": "none",
      "width": 512,
      "height": 512,
      "zoom": 0,
      "transform": "translate(0)"
    },
    "bot": {
      "name": "iAyeBot",
      "email": "iayebot@websemantics.ca",
      "url": "http:\/\/github.com\/iayebot",
      "img": "https:\/\/avatars1.githubusercontent.com\/u\/18201639?v=3&amp;s=40"
    }
  },
  "template": [
    {
      "description": "Web Semantics, Inc. logo and artwork",
      "source": "{{ global.files.source }}\/logo.template",
      "dest": "{{ global.files.dest }}",
      "data":[
        "logo-solid",
        "logo-white",
        "logo-black",
        "logo-yellow",
        "logo-green",
        "logo-blue",
        "logo-red",
        "logo-invert-yellow"
      ],
      "svg": {
        "dest": "svg"
      },
      "png": {
        "dest": "png",
        "scale": {
          "width": [
            200, 400
          ],
          "height": [
            32, 64, 256
          ]
        },
        "retina": true
      },
      "icon": {
        "dest": "icon",
        "source": "{{ global.files.dest }}\/png\/logo-invert-yellow-h256@2x.png",
        "config": {
          "appName": "{{ package.name }}",
          "appDescription": "{{ package.description }}",
          "developerName": "{{ package.author }}",
          "developerURL": "{{ package.homepage }}",
          "background": "{{ global.color.white }}",
          "path": "{{ global.files.dest }}\/icon",
          "url": "{{ package.homepage }}",
          "display": "standalone",
          "orientation": "portrait",
          "version": 1,
          "logging": false,
          "online": false,
          "replace": true
        }
      },
      "sprite": {
        "dest": "sprite",
        "source": "{{ global.files.dest }}\/svg\/*.svg",
        "config": {
          "mode": {
            "view": {
              "bust": false,
              "render": {
                "css": true
              }
            },
            "symbol": true
          }
        }
      }
    }
  ],
  "html": {
    "dest": ".",
    "template": "{{ global.files.source }}\/index.template",
    "filename": "index.html",
    "title": "Web Semantics, Inc. Logos",
    "description": "{{ package.description }}",
    "github_url": "{{& package.repository }}",
    "link": {
      "text": "Visit Website \u00bb",
      "url": "{{& package.repository.url }}"
    },
    "generated_by": {
      "text": "Generated by,",
      "name": "{{ global.bot.name }}",
      "url": "{{& global.bot.url }}",
      "img": "{{& global.bot.img }}"
    }
  },
  "data": {
    "default": {
      "name": "",
      "value": {
        "text": false,
        "fill_1": "{{ global.color.pickled }}",
        "fill_2": "{{ global.color.pickled }}",
        "text_fill": "{{ global.color.pickled }}",
        "logo_transform": "translate(0)",
        "text_transform": "translate(0) scale(1)",
        "zoom": "{{ global.default.zoom }}",
        "width": "{{ global.default.width }}",
        "height": "{{ global.default.height }}",
        "transform": "{{ global.default.transform }}",
        "background_fill": "{{ global.default.none }}"
      }
    },
    "logo": {
      "name": "",
      "children": [
        "default"
      ],
      "value": {

      }
    },
    "text-h": {
      "name": "h",
      "children": [
        "default"
      ],
      "value": {
        "text": true,
        "width": 2900
      }
    },
    "text-v": {
      "name": "v",
      "children": [
        "default"
      ],
      "value": {
        "text": true,
        "width": 1400,
        "height": 800,
        "logo_transform": "translate(442,0)",
        "text_transform": "translate(-341,558) scale(0.6)"
      }
    },
    "logo-solid": {
      "children": [
        "logo",
        "text-h",
        "text-v"
      ],
      "value": {
      }
    },
    "logo-white": {
      "children": [
        "logo",
        "text-h",
        "text-v"
      ],
      "value": {
        "fill_1": "{{ global.color.white }}",
        "fill_2": "{{ global.color.white }}",
        "text_fill": "{{ global.color.white }}"
      }
    },
    "logo-black": {
      "children": [
        "logo",
        "text-h",
        "text-v"
      ],
      "value": {
        "fill_1": "{{ global.color.black }}",
        "fill_2": "{{ global.color.black }}",
        "text_fill": "{{ global.color.black }}"
      }
    },
    "logo-yellow": {
     "children": [
       "logo",
       "text-h",
       "text-v"
     ],
     "value": {
       "fill_1": "{{ global.color.yellow_1 }}",
       "fill_2": "{{ global.color.yellow_2 }}"
     }
    },
    "logo-green": {
     "children": [
       "logo",
       "text-h",
       "text-v"
     ],
     "value": {
       "fill_1": "{{ global.color.green_1 }}",
       "fill_2": "{{ global.color.green_2 }}"
     }
    },
    "logo-blue": {
     "children": [
       "logo",
       "text-h",
       "text-v"
     ],
     "value": {
       "fill_1": "{{ global.color.blue_1 }}",
       "fill_2": "{{ global.color.blue_2 }}"
     }
    },
    "logo-red": {
     "children": [
       "logo",
       "text-h",
       "text-v"
     ],
     "value": {
       "fill_1": "{{ global.color.red_1 }}",
       "fill_2": "{{ global.color.red_2 }}"
     }
    },
    "logo-invert-yellow": {
     "children": [
       "logo"
     ],
     "value": {
       "zoom": "20",
       "text_fill": "{{ global.color.white }}",
       "fill_1": "{{ global.color.white }}",
       "fill_2": "{{ global.color.white }}",
       "background_fill": "{{ global.color.yellow_2 }}"
     }
   }
  }
}
