{
  "global": {
    "color": {
      "white": "#ffffff",
      "black": "#000000",
      "logo": {
          "offical": {
            "c1": "#586069",
            "c2": "#3C4146"
          },
          "cinnabar": {
            "c1": "#e24329",
            "c2": "#fca326"
          },
          "picton": {
            "c1": "#3da9df",
            "c2": "#f27045"
          },
          "elm": {
            "c1": "#1e7889",
            "c2": "#97baa6"
          },
          "blue": {
            "c1": "#3aaeda",
            "c2": "#4fc1e9"
          },
          "green": {
            "c1": "#37bb9b",
            "c2": "#47cfac"
          },
          "orange": {
            "c1": "#e9563e",
            "c2": "#fb6d51"
          },
          "brick": {
            "c1": "#d03349",
            "c2": "#333"
          },
          "grey": {
            "c1": "#c1c1c1",
            "c2": "#a1a1a1"
          }
      }
    },
    "files": {
      "dist": "dist",
      "dest": "assets",
      "source": "src",
      "concatenator": "-"
    },
    "default": {
      "width": 32,
      "height": 32,
      "background_fill": "#f1f1f1",
      "shadow_opacity": ".1",
      "shadow_color": "#111",
      "grey_fill": "#e6e6e6",
      "border_style": "stroke:#000000;stroke-width:0.39506173;stroke-linejoin:round"
    },
    "bot": {
      "name": "iAyeBot",
      "email": "iayebot@websemantics.ca",
      "url": "http:\/\/github.com\/iayebot",
      "img": "https:\/\/avatars1.githubusercontent.com\/u\/18201639?v=3&amp;s=40"
    }
  },
  "template": [
    {
      "description": "Web Semantics, Inc. logo and artwork",
      "source": "{{ global.files.source }}\/logo.template",
      "dest": "{{ global.files.dest }}",
      "data":[
        "logo",
        "icon",
        "logo-white",
        "logo-border-white",
        "logo-black",
        "logo-border-black",
        "logo-cinnabar",
        "logo-picton",
        "logo-elm",
        "logo-blue",
        "logo-green",
        "logo-orange",
        "logo-brick",
        "logo-grey"
      ],
      "svg": {
        "dest": "svg"
      }
    }
  ],
  "html": {
    "dest": ".",
    "template": "{{ global.files.source }}\/index.template",
    "filename": "index.html",
    "title": "Web Semantics, Inc. Logos",
    "description": "{{ package.description }}",
    "github_url": "{{& package.repository }}",
    "link": {
      "text": "Visit Website \u00bb",
      "url": "{{& package.repository.url }}"
    },
    "generated_by": {
      "text": "Generated by,",
      "name": "{{ global.bot.name }}",
      "url": "{{& global.bot.url }}",
      "img": "{{& global.bot.img }}"
    }
  },
  "data": {
    "default": {
      "name": "",
      "value": {
        "zoom": 0,
        "style": false,
        "solid": false,
        "edge": false,
        "shadow": false,
        "grey_arms": false,
        "background_rx": 0,
        "background_ry": 0,
        "background": false,
        "variant_left": false,
        "variant_right": false,
        "transform": "translate(0)",
        "width": "{{ global.default.width }}",
        "height": "{{ global.default.height }}",
        "grey_fill": "{{ global.default.grey_fill }}",
        "shadow_opacity": "{{ global.default.shadow_opacity }}",
        "shadow_color": "{{ global.default.shadow_color }}"
      }
    },
    "background": {
      "name": "bg",
      "children": [
        "default"
      ],
      "value": {
        "zoom": 10,
        "background": "{{ global.default.background_fill }}"
      }
    },
    "base": {
      "children": [
        "default",
        "background"
      ],
      "value": {
      }
    },
    "shadow": {
      "name": "sw",
      "children": [
        "base"
      ],
      "value": {
        "shadow": true
      }
    },
    "border": {
      "name": "b",
      "children": [
        "base"
      ],
      "value": {
        "style": "{{ global.default.border_style }}"
      }
    },
    "edge": {
      "children": [
        "base"
      ],
      "value": {
        "edge": 1
      }
    },
    "grey": {
      "name": "g",
      "children": [
        "base",
        "shadow"
      ],
      "value": {
        "grey_arms": true
      }
    },
    "solid": {
      "children": [
        "base"
      ],
      "value": {
        "solid": true
      }
    },
    "left": {
      "name": "l",
      "children": [
        "base",
        "shadow",
        "grey"
      ],
      "value": {
        "variant_left": true
      }
    },
    "left-grey": {
      "name": "lg",
      "children": [
        "grey"
      ],
      "value": {
        "variant_left": true
      }
    },
    "right": {
      "name": "r",
      "children": [
        "base",
        "shadow",
        "grey"
      ],
      "value": {
        "variant_right": true
      }
    },
    "right-grey": {
      "name": "rg",
      "children": [
        "grey"
      ],
      "value": {
        "variant_right": true
      }
    },
    "full": {
      "name": "",
      "children": [
        "base",
        "edge",
        "shadow",
        "grey",
        "right",
        "left"
      ],
      "value": {
      }
    },
    "logo": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{#color}} ('{{ global.color.logo.offical.c2 }}').lighten(35) {{/color}}",
        "bottom_left_fill": "{{ global.color.logo.offical.c1 }}",
        "bottom_right_fill": "{{ global.color.logo.offical.c2 }}",
        "arm_right_fill": "{{#color}} ('{{ global.color.logo.offical.c2 }}').lighten(35) {{/color}}"
      }
    },
    "icon": {
      "children": [
        "default"
      ],
      "value": {
        "zoom": 35,
        "solid": true,
        "background_rx": 30,
        "background_ry": 30,
        "background": "{{ global.color.logo.offical.c2 }}",
        "arm_left_fill": "{{ global.color.white }}"
      }
    },
    "logo-white": {
      "children": [
        "solid",
        "edge",
        "right-grey",
        "left-grey"
      ],
      "value": {
        "arm_left_fill": "#fff",
        "bottom_left_fill": "#fff",
        "bottom_right_fill": "#fff",
        "arm_right_fill": "#fff"
      }
    },
    "logo-border-white": {
      "children": [
        "solid",
        "right-grey",
        "left-grey"
      ],
      "value": {
        "zoom": 1,
        "style": "{{ global.default.border_style }}",
        "arm_left_fill": "{{ global.color.white }}",
        "bottom_left_fill": "{{ global.color.white }}",
        "bottom_right_fill": "{{ global.color.white }}",
        "arm_right_fill": "{{ global.color.white }}"
      }
    },
    "logo-black": {
      "children": [
        "solid",
        "edge",
        "right-grey",
        "left-grey"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.black }}",
        "bottom_left_fill": "{{ global.color.black }}",
        "bottom_right_fill": "{{ global.color.black }}",
        "arm_right_fill": "{{ global.color.black }}"
      }
    },
    "logo-border-black": {
      "children": [
        "solid",
        "right-grey",
        "left-grey"
      ],
      "value": {
        "zoom": 1,
        "style": "{{ global.default.border_style }}",
        "arm_left_fill": "{{ global.color.black }}",
        "bottom_left_fill": "{{ global.color.black }}",
        "bottom_right_fill": "{{ global.color.black }}",
        "arm_right_fill": "{{ global.color.black }}"
      }
    },
    "logo-cinnabar": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.cinnabar.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.cinnabar.c2 }}",
        "bottom_right_fill": "{{#color}} ('{{ global.color.logo.cinnabar.c2 }}').saturate(0).spin(-15) {{/color}}",
        "arm_right_fill": "{{ global.color.logo.cinnabar.c1 }}"
      }
    },
    "logo-picton": {
      "children": [
        "solid",
        "right",
        "left"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.picton.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.picton.c2 }}",
        "bottom_right_fill": "{{#color}} ('{{ global.color.logo.picton.c2 }}').saturate(0).spin(-15) {{/color}}",
        "arm_right_fill": "{{ global.color.logo.picton.c1 }}"
      }
    },
    "logo-elm": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.elm.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.elm.c2 }}",
        "bottom_right_fill": "{{ global.color.logo.elm.c2 }}",
        "arm_right_fill": "{{ global.color.logo.elm.c1 }}"
      }
    },
    "logo-blue": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.blue.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.blue.c2 }}",
        "bottom_right_fill": "{{ global.color.logo.blue.c2 }}",
        "arm_right_fill": "{{ global.color.logo.blue.c1 }}"
      }
    },
    "logo-green": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.green.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.green.c2 }}",
        "bottom_right_fill": "{{ global.color.logo.green.c2 }}",
        "arm_right_fill": "{{ global.color.logo.green.c1 }}"
      }
    },
    "logo-orange": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.orange.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.orange.c2 }}",
        "bottom_right_fill": "{{ global.color.logo.orange.c2 }}",
        "arm_right_fill": "{{ global.color.logo.orange.c1 }}"
      }
    },
    "logo-brick": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.brick.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.brick.c2 }}",
        "bottom_right_fill": "{{ global.color.logo.brick.c2 }}",
        "arm_right_fill": "{{ global.color.logo.brick.c1 }}"
      }
    },
    "logo-grey": {
      "children": [
        "solid",
        "full"
      ],
      "value": {
        "arm_left_fill": "{{ global.color.logo.grey.c1 }}",
        "bottom_left_fill": "{{ global.color.logo.grey.c2 }}",
        "bottom_right_fill": "{{ global.color.logo.grey.c2 }}",
        "arm_right_fill": "{{ global.color.logo.grey.c1 }}"
      }
    }
  }
}
